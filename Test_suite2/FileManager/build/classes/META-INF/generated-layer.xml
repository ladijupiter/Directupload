                        <attr intvalue="1" name="persistenceType"/>
                        <attr intvalue="1000" name="position"/>
                        <attr name="method" stringvalue="createEditor"/>
                        <attr boolvalue="true" name="sourceview"/>
                    </file>
                    <file name="org-filemanger-aj-aspectFileTypeVisualElement.instance">
                        <!--org.filemanger.aj.aspectFileTypeVisualElement-->
